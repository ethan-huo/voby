{
  "name": "voby",
  "repository": "github:ethan-huo/voby",
  "description": "A high-performance framework with fine-grained observable/signal-based reactivity for building rich applications.",
  "version": "0.58.1",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx/runtime.d.ts",
      "import": "./dist/jsx/runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx/runtime.d.ts",
      "import": "./dist/jsx/runtime.js"
    },
    "./router": {
      "types": "./dist/router/index.d.ts",
      "import": "./dist/router/index.js"
    },
    "./build/vite": {
      "types": "./dist/build/vite.d.ts",
      "import": "./dist/build/vite.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist node_modules/.tmp/tsconfig.build.tsbuildinfo && bun tsc -p tsconfig.build.json",
    "test": "bun vitest",
    "test:run": "bun vitest run",
    "bench:core": "bun scripts/bench/core-bench.ts",
    "memory:core": "bun scripts/memory/core-memory.ts",
    "memory:core-oby": "bun scripts/memory/core-memory-oby.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ui",
    "framework",
    "reactive",
    "observable",
    "signal",
    "fast",
    "performant",
    "performance",
    "small",
    "fine-grained",
    "updates"
  ],
  "dependencies": {
    "htm": "^3.1.1",
    "oby": "^15.1.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "happy-dom": "^20.1.0",
    "lefthook": "^2.0.13",
    "typescript": "^5.9.3",
    "valibot": "^1.2.0",
    "vitest": "^4.0.17",
    "vite": "^7.3.0"
  }
}
